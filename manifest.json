{
  "author": "Zenquan",
  "background": {
    "page": "githubHelper.html"
  },
  "content_scripts": [
    {
      "js": ["js/content-script.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "a quick git clone crx",
    "default_popup": "githubHelper.html"
  },
  "description": "这是一个帮助快速克隆git仓库的Chrome插件",
  "manifest_version": 2,
  "name": "githubHelper",
  "short_name": "githubHelper",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "version": "1.0"
}